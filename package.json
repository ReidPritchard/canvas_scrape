{
  "$schema": "http://json.schemastore.org/package",
  "name": "canvas_scrape",
  "bin": "./main.js",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "main.js",
  "dependencies": {
    "@doist/todoist-api-typescript": "^5.5.1",
    "@dotenvx/dotenvx": "^1.51.0",
    "@notionhq/client": "^0.3.1",
    "chrono-node": "^2.3.1",
    "dotenv": "^17.2.2",
    "playwright": "^1.8.0",
    "todoist": "^0.3.0",
    "winston": "^3.17.0"
  },
  "scripts": {
    "setup": "node setup.js",
    "dev": "node main.js --dev",
    "dev:skip-scraping": "node main.js --dev --skip-scraping",
    "build": "rm -rf dist && node build.js",
    "package": "pnpm run build && pkg dist/main.cjs --targets node22-macos-x64,node22-linux-x64 --output dist/canvas-scrape --compress Brotli && cp .env dist/.env",
    "format": "npx prettier --write .",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.0.0",
  "devDependencies": {
    "@vitest/coverage-v8": "^3.2.4",
    "@yao-pkg/pkg": "^6.7.0",
    "esbuild": "0.25.10",
    "vitest": "^3.2.4"
  }
}
